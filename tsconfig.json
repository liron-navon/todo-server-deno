{
    "compilerOptions": {
      "allowJs": true,
      "baseUrl": "./",
      "esModuleInterop": true,
      "lib": ["esnext"],
      "module": "esnext",
      "moduleResolution": "node",
      "noEmit": true,
      "noImplicitAny": false,
      "paths": {
        "http://*": ["../../../../Library/Caches/deno/deps/http/*"],
        "https://*": ["../../../../Library/Caches/deno/deps/https/*"]
      },
      "plugins": [ { "name": "typescript-deno-plugin" } ],
      "pretty": true,
      "resolveJsonModule": true,
      "target": "esnext"
    },
    "include": [
      "./**/*.ts"
    ]
  }